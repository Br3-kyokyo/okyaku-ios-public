create_table "trigger_keywords", force: :cascade do |t|
  t.string "word"
  t.integer "trigger_id", null: false
  t.datetime "created_at", precision: 6, null: false
  t.datetime "updated_at", precision: 6, null: false
  t.index ["trigger_id"], name: "index_trigger_keywords_on_trigger_id"
end
